{
    "manifest_version": 3,
    "name": "Gugel",
    "version": "1.0.0",
    "action": {
        "default_popup": "dist/index.html"
    },
    "permissions": [
        "storage",
        "activeTab",
        "scripting",
        "tabs",
        "cookies"
    ],
    "host_permissions": [
        "http://localhost/*"
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "dist/assets/*"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ]
}



