{
    "manifest_version": 3,
    "name": "Nano Playground",
    "version": "0.0.1",
    "action": {
        "default_popup": "index.html"
    },
    "permissions": [
        "storage",
        "activeTab",
        "scripting",
        "tabs",
        "cookies"
    ],
    "host_permissions": [
        "http://localhost/*"
    ]
}



